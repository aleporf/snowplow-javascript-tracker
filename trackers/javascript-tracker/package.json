{
  "name": "@snowplow/javascript-tracker",
  "version": "2.17.3",
  "private": true,
  "description": "JavaScript tracker for Snowplow",
  "keywords": [
    "tracking",
    "web analytics",
    "events",
    "open source"
  ],
  "bugs": "https://github.com/snowplow/snowplow-javascript-tracker/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/snowplow/snowplow-javascript-tracker.git"
  },
  "license": "Simplified BSD",
  "contributors": [
    "Alex Dean",
    "Simon Andersson",
    "Anthon Pang",
    "Fred Blundun",
    "Joshua Beemster",
    "Michael Hadam",
    "Paul Boocock"
  ],
  "main": "dist/sp.js",
  "scripts": {
    "build": "rollup -c --silent --failAfterWarnings",
    "docker:micro": "docker pull snowplow/snowplow-micro:1.1.0",
    "test": "jest test/unit/*.test.ts",
    "test:build": "rollup --config rollup.config.test.js --silent",
    "test:e2e:local": "npm-run-all --parallel test:build docker:micro --serial wdio:local",
    "test:e2e:sauce": "npm-run-all --parallel test:build docker:micro --serial wdio:sauce",
    "wdio:local": "wdio test/wdio.local.conf.js",
    "wdio:sauce": "wdio test/wdio.sauce.conf.js"
  },
  "dependencies": {
    "@snowplow/browser-core": "workspace:*",
    "@snowplow/browser-plugin-browser-features": "workspace:*",
    "@snowplow/browser-plugin-client-hints": "workspace:*",
    "@snowplow/browser-plugin-error-tracking": "workspace:*",
    "@snowplow/browser-plugin-form-tracking": "workspace:*",
    "@snowplow/browser-plugin-ga-cookies": "workspace:*",
    "@snowplow/browser-plugin-gdpr": "workspace:*",
    "@snowplow/browser-plugin-geolocation": "workspace:*",
    "@snowplow/browser-plugin-link-click-tracking": "workspace:*",
    "@snowplow/browser-plugin-optimizely": "workspace:*",
    "@snowplow/browser-plugin-optimizely-x": "workspace:*",
    "@snowplow/browser-plugin-parrable": "workspace:*",
    "@snowplow/browser-plugin-performance-timing": "workspace:*",
    "@snowplow/browser-plugin-timezone": "workspace:*",
    "@snowplow/browser-tracker": "workspace:*",
    "@snowplow/tracker-core": "workspace:*",
    "lodash": "^4.17.20",
    "tslib": "^2.0.3"
  },
  "devDependencies": {
    "@ampproject/rollup-plugin-closure-compiler": "^0.26.0",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@types/dockerode": "^3.2.2",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.6.0",
    "@wdio/cli": "^6.12.1",
    "@wdio/jasmine-framework": "^6.11.0",
    "@wdio/local-runner": "^6.12.1",
    "@wdio/sauce-service": "^6.12.1",
    "@wdio/spec-reporter": "^6.11.0",
    "@wdio/static-server-service": "^6.10.10",
    "@wdio/sync": "^6.11.0",
    "@wessberg/rollup-plugin-ts": "^1.3.8",
    "chromedriver": "^88.0.0",
    "dockerode": "^3.2.1",
    "gulp": "^4.0.2",
    "jest": "^26.6.3",
    "jest-standard-reporter": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.34.0",
    "rollup-plugin-banner": "^0.2.1",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-sizes": "^1.0.3",
    "saucelabs": "^4.6.0",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2",
    "wdio-chromedriver-service": "^6.0.4"
  }
}
